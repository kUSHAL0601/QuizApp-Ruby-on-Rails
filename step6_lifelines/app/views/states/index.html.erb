<p id="notice"><%= notice %></p>

<h1>States</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Genre</th>
      <th>Subgenre</th>
      <th>Qsnid</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <%if @states!=nil%>
    <% @states.each do |state| %>
      <tr>
        <td><%= state.userid %></td>
        <td><%= state.genre %></td>
        <td><%= state.subgenre %></td>
        <td><%= state.qsnid %></td>
        <td><%= state.score %></td>
        <td><%= link_to 'Show', state %></td>
        <td><%= link_to 'Edit', edit_state_path(state) %></td>
        <td><%= link_to 'Destroy', state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <%end%>
  </tbody>
</table>

<br>

<%= link_to 'New State', new_state_path %>
